{
    "name": "carllee1983/newebpay-logistics",
    "description": "NewebPay Logistics Integration PHP SDK. Supports various logistics methods including CVS pickup, home delivery, and more.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Carl Lee",
            "email": "carllee1983@gmail.com"
        }
    ],
    "homepage": "https://github.com/CarlLee1983/newebpay-logistics",
    "support": {
        "issues": "https://github.com/CarlLee1983/newebpay-logistics/issues",
        "source": "https://github.com/CarlLee1983/newebpay-logistics"
    },
    "keywords": [
        "newebpay",
        "logistics",
        "taiwan",
        "shipping",
        "cvs",
        "convenience-store",
        "laravel",
        "sdk"
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^1.1 || ^3.0"
    },
    "require-dev": {
        "illuminate/support": "^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.4",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.7 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "CarlLee\\NewebPayLogistics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CarlLee\\NewebPayLogistics\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarlLee\\NewebPayLogistics\\Laravel\\NewebPayLogisticsServiceProvider"
            ],
            "aliases": {
                "NewebPayLogistics": "CarlLee\\NewebPayLogistics\\Laravel\\Facades\\NewebPayLogistics"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "analyse": "phpstan analyse --memory-limit=1G",
        "check": [
            "@cs",
            "@analyse",
            "@test"
        ],
        "format": "phpcbf"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}