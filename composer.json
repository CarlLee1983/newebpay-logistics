{
    "name": "carllee1983/newebpay-logistics",
    "description": "NewebPay Logistics Integration PHP SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Carl Lee",
            "email": "carllee1983@gmail.com"
        }
    ],
    "keywords": [
        "newebpay",
        "logistics",
        "taiwan",
        "shipping",
        "cvs",
        "convenience-store"
    ],
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "illuminate/support": "^12.41",
        "mockery/mockery": "^1.4",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "CarlLee\\NewebPayLogistics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CarlLee\\NewebPayLogistics\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarlLee\\NewebPayLogistics\\Laravel\\NewebPayLogisticsServiceProvider"
            ],
            "aliases": {
                "NewebPayLogistics": "CarlLee\\NewebPayLogistics\\Laravel\\Facades\\NewebPayLogistics"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "analyse": "phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
